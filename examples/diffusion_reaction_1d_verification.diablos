    "_verification_notes": {
        "problem": "1D Diffusion-Reaction with LINEAR decay (n=1)",
        "equation": "dc/dt = D*d\u00b2c/dx\u00b2 - k*c",
        "initial_condition": "c(x,0) = sin(pi*x/L)",
        "boundary_conditions": "c(0) = c(L) = 0 (Dirichlet)",
        "analytical_solution": "c(x,t) = sin(pi*x/L) * exp(-(D*(pi/L)\u00b2 + k)*t)",
        "parameters": {
            "D": 0.1,
            "k": 0.5,
            "L": 1.0,
            "n": 1
        },
        "at_center": "c(0.5,t) = 1.0 * exp(-(0.1*pi\u00b2 + 0.5)*t) = exp(-1.487*t)",
        "total_decay_rate": "lambda = D*(pi/L)\u00b2 + k = 0.1*9.87 + 0.5 = 1.487",
        "expected_values": {
            "t=0": 1.0,
            "t=1": 0.226,
            "t=2": 0.051,
            "t=5": 0.0006
        },
        "note": "This verification only works for LINEAR reaction (n=1). For nonlinear reaction (n\u22601), analytical solutions are generally not available."
    }
